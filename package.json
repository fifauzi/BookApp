    "intl": "^1.2.5",
    "react": "17.0.2",
    "react-native": "0.68.0",
    "react-native-blob-util": "^0.16.2",
    "react-native-dotenv": "^3.3.1",
    "react-native-elements": "^3.4.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-push-notification": "^8.1.1",
